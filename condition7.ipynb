    "def count_range(li,min,max):\n",
    "    ctr=0\n",
    "    for x in li:\n",
    "        if min<=x<=max:\n",
    "            ctr+=1\n",
    "    return ctr\n",
    "li=[2,43,30,56,54,34,76,89,90]\n",
    "print(count_range(li,31,100))"
   ]
